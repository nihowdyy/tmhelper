{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "autoIncrement": true,
        "buildType": "app-bundle",        
        "env": {
          "EXPO_ANDROID_TARGET_SDK_VERSION": "34"
        }
      }
    }
  }
}